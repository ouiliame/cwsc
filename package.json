{
  "name": "cwsc",
  "version": "1.0.0",
  "description": "Official CWScript compiler",
  "main": "dist/bundle.js",
  "module": "dist/module.js",
  "type": "commonjs",
  "types": "dist/index.d.ts",
  "author": "William Chen",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/prettier": "^2.7.0",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "assert": "^2.1.0",
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "bun": "^1.0.18",
    "bunjs": "^1.5.1",
    "filer": "^1.4.1",
    "jest": "^29.7.0",
    "process": "^0.11.10",
    "shx": "^0.3.4",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "zod-to-json-schema": "^3.22.1"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "antlr4ts": "^0.5.0-alpha.4",
    "handlebars": "^4.7.8",
    "immutable": "^5.0.0-beta.4",
    "jinx-rust": "^0.1.6",
    "lines-and-columns": "^2.0.4",
    "path-browserify": "^1.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-rust": "^0.1.9",
    "stream-browserify": "^3.0.0",
    "vscode-languageserver": "^9.0.1",
    "zod": "^3.22.4"
  },
  "scripts": {
    "antlr-lexer": "antlr4ts ./grammar/CWScriptLexer.g4 -o ./src",
    "antlr-parser": "antlr4ts -visitor ./grammar/CWScriptParser.g4 -o ./src -lib ./src/grammar",
    "antlr": "bun run antlr-lexer && bun run antlr-parser",
    "format": "prettier --check ./src/**/*.ts",
    "format:fix": "prettier --write ./src/**/*.ts",
    "build:json-schema": "ts-node ./src/generate-cwsproject-json-schema.ts",
    "build": "shx rm -rf dist && tsc --module commonjs && webpack --mode production",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint --fix ./src/**/*.ts",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prepublishOnly": "yarn run build",
    "e2e": "ts-node src/e2e.ts",
    "e2e-build": "yarn run e2e && cd examples/kitchen-sink/build/kitchen-sink && cargo wasm",
    "dev": "cd cws-devtool && yarn start",
    "test": "jest"
  },
  "files": [
    "bin",
    "dist",
    "src",
    "cwsproject.schema.json"
  ],
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
